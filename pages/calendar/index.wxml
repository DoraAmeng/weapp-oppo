<view>
  <scroll-view scroll-top="{{scrollTop}}" scroll-y="true" style="height:{{scrollHeight}}px;" class="list" bindscrolltolower="bindDownLoad" bindscrolltoupper="topLoad" bindscroll="scroll">
    <view>
      <view wx:for="{{allResList}}" wx:key="index">
        <view class='calendar-line'>{{item.time}}</view>
        <view class='img-list'>
          <image mode="aspectFill" src='{{itemImg.img}}' data-src="{{itemImg.img}}" wx:for="{{item.data}}" wx:for-item="itemImg" wx:key="index" style="width: {{width}}px;height: {{height}}px;" bindtap="previewImg"></image>
        </view>
      </view>
    </view>
    <view class='calendarOverTip' wx:if='{{imgNumEndTips}}'>
      <image src='../../image/wocao.jpg'></image>
      <view class='tips-txt'>所有的壁纸都被你看完了，挺牛批的啊！</view>
    </view>
  </scroll-view>
  <view class="loading" wx:if="{{loading}}"></view>
</view>