.app-body {
  text-align: center;
}

.logo {
  display: block;
  width: 240rpx;
  height: 84rpx;
  padding: 34rpx 0 0;
  margin: 0 auto;
}

.app-title {
  color: #8a8a8a;
  font-size: 30rpx;
  margin: 0 auto 50rpx;
}

.page-title {
  position: relative;
  font-size: 36rpx;
  margin: 0 80rpx;
  font-weight: bold;
}

.page-title::before {
  content: '';
  position: absolute;
  left: -60rpx;
  top: 1rpx;
  width: 50rpx;
  height: 50rpx;
  background: transparent url(http://bmob-cdn-21525.b0.upaiyun.com/2018/09/14/94c6a61440b725f480c3e6ddc826b716.png) no-repeat center;
  background-size: cover;
}

.page-title::after {
  content: '';
  position: absolute;
  right: -60rpx;
  top: 1rpx;
  width: 50rpx;
  height: 50rpx;
  background: transparent url(http://bmob-cdn-21525.b0.upaiyun.com/2018/09/14/94c6a61440b725f480c3e6ddc826b716.png) no-repeat center;
  background-size: cover;
  transform: rotateY(180deg);
}

.image-con {
  clear: both;
  margin: 50rpx 0;
}

.image-con view {
  display: block;
  font-size: 24rpx;
  color: #8a8a8a;
  margin-bottom: 20rpx;
}

.logo1 {
  display: inline-block;
  width: 240rpx;
  height: 240rpx;
  margin: 20rpx auto;
  background: #fff;
  box-shadow: 0 0 50rpx #c6d8ff;
}

.logo2 {
  display: inline-block;
  width: 240rpx;
  height: 84rpx;
  margin: 20rpx auto;
  background: #fff;
  box-shadow: 0 0 50rpx #c6d8ff;
}

.color-card {
  margin: 50rpx 30rpx;
}

.color-card-bg {
  height: 100rpx;
  background: #1f81e3;
  padding: 30rpx;
}

.color-card-bg view {
  position: relative;
  color: #fff;
  text-align: left;
  font-size: 24rpx;
}

.color-card-bg view text {
  position: absolute;
  left: 50%;
  color: #fff;
  text-align: left;
}

.color-card-title {
  font-size: 24rpx;
  margin: 10rpx;
  text-align: left;
}

.color-card-title view {
  font-size: 24rpx;
  color: #b4b4b4;
  text-align: left;
}

.color-card-bg2 {
