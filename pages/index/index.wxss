/**index.wxss**/

.image-con {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
}

.change-img-con {
  position: relative;
  width: 100%;
  height: 100vh;
  text-align: center;
}

.change-img {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 100;
  width: 480rpx;
  height: 640rpx;
  margin: 0 auto;
  border-radius: 40rpx;
}
