.emoji--sad .emoji__mouth {
  width: 60rpx;
  height: 80rpx;
  left: calc(50% - 30rpx);
  top: 80rpx;
  box-sizing: border-box;
  border: 6rpx solid #000;
  border-radius: 50%;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  background: transparent;
  animation: sad-mouth 2s ease-in infinite;
}

.emoji--sad .emoji__mouth:after {
  width: 6rpx;
  height: 6rpx;
  background: transparent;
  border-radius: 50%;
  top: 4rpx;
  left: calc(50% - 3rpx);
  box-shadow: -18rpx 0 0 0 #000, 18rpx 0 0 0 #000;
}

.emoji--angry {
  background: linear-gradient(#d5234c -10%, #ffda6a);
  background-size: 100%;
  animation: angry-color 2s ease-in infinite;
}

.emoji--angry:after {
  content: "讨厌";
}

.emoji--angry .emoji__face {
  animation: angry-face 2s ease-in infinite;
}

.emoji--angry .emoji__eyebrows {
  left: calc(50% - 3rpx);
  top: 55rpx;
  height: 6rpx;
  width: 6rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: -44rpx 5rpx 0 0 #000,
                -7rpx 16rpx 0 0 #000,
                7rpx 16rpx 0 0 #000,
                44rpx 5rpx 0 0 #000;
}

.emoji--angry .emoji__eyebrows:before, .emoji--angry .emoji__eyebrows:after {
  width: 50rpx;
  height: 20rpx;
  border: 6rpx solid #000;
  box-sizing: border-box;
  border-radius: 50%;
  border-top-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  top: 0;
  left: calc(50% - 25rpx);
}

.emoji--angry .emoji__eyebrows:before {
  margin-left: -25rpx;
  transform: rotate(15deg);
}

.emoji--angry .emoji__eyebrows:after {
  margin-left: 25rpx;
  transform: rotate(-15deg);
}

.emoji--angry .emoji__eyes {
  width: 12rpx;
  height: 12rpx;
  left: calc(50% - 6rpx);
  top: 70rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: 25rpx 0 0 0 #000, -25rpx 0 0 0 #000;
}

.emoji--angry .emoji__mouth {
  width: 36rpx;
  height: 18rpx;
  left: calc(50% - 18rpx);
  bottom: 15rpx;
  background: #000;
  border-radius: 50%;
  animation: angry-mouth 2s ease-in infinite;
}

@keyframes heart-beat {
  25% {
    transform: scale(1.1);
  }

  75% {
    transform: scale(0.6);
  }
}

@keyframes haha-face {
  10%, 30%, 50% {
    transform: translateY(25rpx);
  }

  20%, 40% {
    transform: translateY(15rpx);
  }

  60%, 80% {
    transform: translateY(0);
  }

  70%, 90% {
    transform: translateY(-10rpx);
  }
}

@keyframes haha-mouth {
  10%, 30%, 50% {
    transform: scale(0.6);
    top: 45%;
  }

  20%, 40% {
    transform: scale(0.8);
    top: 45%;
  }

  60%, 80% {
    transform: scale(1);
    top: 50%;
  }

  70% {
    transform: scale(1.2);
    top: 50%;
  }

  90% {
    transform: scale(1.1);
    top: 50%;
  }
}

@keyframes yay {
  25% {
    transform: rotate(-15deg);
  }

  75% {
    transform: rotate(15deg);
  }
}

@keyframes wow-face {
  15%, 25% {
    transform: rotate(20deg) translateX(-25rpx);
  }

  45%, 65% {
    transform: rotate(-20deg) translateX(25rpx);
  }

  75%, 100% {
    transform: rotate(0deg) translateX(0);
  }
}

@keyframes wow-brow {
  15%, 65% {
    top: 25rpx;
  }

  75%, 100%, 0% {
    top: 15rpx;
  }
}

@keyframes wow-mouth {
  10%, 30% {
    width: 20rpx;
    height: 20rpx;
    left: calc(50% - 10rpx);
  }

  50%, 70% {
    width: 30rpx;
    height: 40rpx;
    left: calc(50% - 15rpx);
  }

  75%, 100% {
    height: 50rpx;
  }
}

@keyframes sad-face {
  25%, 35% {
    top: -15rpx;
  }

  55%, 95% {
    top: 10rpx;
  }

  100%, 0% {
    top: 0;
  }
}

@keyframes sad-mouth {
  25%, 35% {
    transform: scale(0.85);
    top: 70rpx;
  }

  55%, 100%, 0% {
    transform: scale(1);
    top: 80rpx;
  }
}

@keyframes tear-drop {
  0%, 100% {
    display: block;
    left: 35rpx;
    top: 15rpx;
    transform: rotate(45deg) scale(0);
  }

  25% {
    display: block;
    left: 35rpx;
    transform: rotate(45deg) scale(2);
  }

  49.9% {
    display: block;
    left: 35rpx;
    top: 65rpx;
    transform: rotate(45deg) scale(0);
  }

  50% {
    display: block;
    left: -35rpx;
    top: 15rpx;
    transform: rotate(45deg) scale(0);
  }

  75% {
    display: block;
    left: -35rpx;
    transform: rotate(45deg) scale(2);
  }

  99.9% {
    display: block;
    left: -35rpx;
    top: 65rpx;
    transform: rotate(45deg) scale(0);
  }
}

@keyframes hands-up {
  25% {
    transform: rotate(15deg);
  }

  50% {
    transform: rotate(-15deg) translateY(-10rpx);
  }

  75%, 100% {
    transform: rotate(0deg);
  }
}

@keyframes thumbs-up {
  25% {
    transform: rotate(20deg);
  }

  50%, 100% {
    transform: rotate(5deg);
  }
}

@keyframes angry-color {
  45%, 60% {
    background-size: 250%;
  }

  85%, 100%, 0% {
    background-size: 100%;
  }
}

@keyframes angry-face {
  35%, 60% {
    transform: translateX(0) translateY(10rpx) scale(0.9);
  }

  40%, 50% {
    transform: translateX(-5rpx) translateY(10rpx) scale(0.9);
  }

  45%, 55% {
    transform: translateX(5rpx) translateY(10rpx) scale(0.9);
  }
}

@keyframes angry-mouth {
  25%, 50% {
    height: 6rpx;
    bottom: 25rpx;
  }
}

.emoji-love-area {
  position: absolute;
  right: 10rpx;
  width: 150rpx;
  z-index: 1001;
  transform: translateX(160rpx);
  transition: all 0.2s ease;
}

.emoji-sad-area {
  position: absolute;
  left: 10rpx;
  width: 150rpx;
  z-index: 1001;
  transform: translateX(-160rpx);
  transition: all 0.2s ease;
}

.emoji-hover {
  transform: scale(1.2);
  transition: all 0.2s ease;
  opacity: 1;
}

.sad-area-hover {
  transform: translateX(0rpx);
  transition: all 0.2s ease;
}

.love-area-hover {
  transform: translateX(0rpx);
  transition: all 0.2s ease;
}

.bottomTip {
  position: absolute;
  bottom: 110rpx;
  width: 284rpx;
  height: 50rpx;
  background: rgba(60, 219, 100, 0.384);
  border-radius: 20rpx 0 20rpx 20rpx;
  color: #33c467;
  font-size: 26rpx;
  line-height: 50rpx;
  text-align: center;
  font-weight: bold;
}

.overTip {
  display: none;
  font-size: 30rpx;
  width: 400rpx;
  text-align: center;
}

.calendar {
  position: absolute;
  top: 30rpx;
  right: 50rpx;
  text-align: center;
  font-size: 22rpx;
  color: #33c467;
  font-weight: bold;  
  z-index: 1002;
}

.calendar image {
  display: block;
  margin: 0 auto;
  width: 32px;
  height: 32px;
}

.navigator-hover {
	background: transparent;
}