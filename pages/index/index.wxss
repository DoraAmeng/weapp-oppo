/**index.wxss**/

.image-con {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
}

.change-img-area {
  position: relative;
  width: 100%;
  height: 100vh;
  text-align: center;
  z-index: 1000;
}

.change-img-area2 {
  position: absolute;
  width: 100%;
  height: 100vh;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999;
}

.change-img-view {
  width: 560rpx;
  height: 800rpx;
}

.change-img-view2 {
  width: 560rpx;
  height: 800rpx;
  transition: all 0.6s ease;
}

.change-img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  border-radius: 40rpx;
  transform-origin: 50% 0;
}

.change-img-active {
  z-index: 100;
  transform: scale(1) translateY(0);
}

.change-img-area2 .change-img:nth-of-type(1) {
  z-index: 99;
  transform: scale(0.9) translateY(-10px);
}

.change-img-area2 .change-img:nth-of-type(2) {
  z-index: 98;
  transform: scale(0.8) translateY(-20px);
}

.change-img-area2 .change-img:nth-of-type(3) {
  z-index: 97;
  transform: scale(0.7) translateY(-30px);
}

.change-img-title {
  position: absolute;
  bottom: 60rpx;
  left: 0;
  font-size: 24rpx;
  z-index: 101;
  padding: 0 25rpx;
  color: #fff;
  text-align: left;
  transition: all 0.6s ease;
}

.change-img-author {
  position: absolute;
  bottom: 20rpx;
  left: 0;
  font-size: 24rpx;
  z-index: 101;
  padding: 0 25rpx;
  color: #fff;
  text-align: left;
  transition: all 0.6s ease;
}

.change-img-time {
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  font-size: 24rpx;
  z-index: 101;
  padding: 2rpx 12rpx 4rpx;
  background: rgba(255, 255, 255, 0.384);
  color: #fff;
  border-radius: 20rpx 20rpx 20rpx 0;
  transition: all 0.6s ease;
}

.top {
  position: absolute;
  top: 34rpx;
}

.top image {
  display: block;
  width: 300rpx;
  height: 92rpx;
}

.bottom {
  position: absolute;
  bottom: 34rpx;
  width: 450rpx;
  font-size: 22rpx;
  text-align: center;
  color: #b4b4b4;
}

.emoji {
  width: 120rpx;
  height: 120rpx;
  margin: 15rpx 15rpx 40rpx;
  background: #ffda6a;
  display: inline-block;
  border-radius: 50%;
  position: relative;
  transition: all 0.2s ease;
  opacity: 0.5;
}

.emoji:after {
  position: absolute;
  bottom: -40rpx;
  font-size: 24rpx;
  width: 60rpx;
  left: calc(50% - 24rpx);
  color: #000;
}

.emoji__face, .emoji__eyebrows, .emoji__eyes, .emoji__mouth, .emoji__tongue,
.emoji__heart, .emoji__hand, .emoji__thumb {
  position: absolute;
}

.emoji__face:before, .emoji__face:after, .emoji__eyebrows:before,
.emoji__eyebrows:after, .emoji__eyes:before, .emoji__eyes:after,
.emoji__mouth:before, .emoji__mouth:after, .emoji__tongue:before,
.emoji__tongue:after, .emoji__heart:before, .emoji__heart:after,
.emoji__hand:before, .emoji__hand:after, .emoji__thumb:before,
.emoji__thumb:after {
  position: absolute;
  content: "";
}

.emoji__face {
  width: inherit;
  height: inherit;
}

.emoji--like {
  background: rgb(231, 79, 68);
}

.emoji--like:after {
  content: "点赞";
}

.emoji--like .emoji__hand {
  left: 25rpx;
  bottom: 30rpx;
  width: 20rpx;
  height: 40rpx;
  background: #fff;
  border-radius: 5rpx;
  z-index: 0;
  animation: hands-up 2s linear infinite;
}

.emoji--like .emoji__hand:before {
  left: 25rpx;
  bottom: 5rpx;
  width: 40rpx;
  background: inherit;
  height: 10rpx;
  border-radius: 2rpx 10rpx 10rpx 2rpx;
  box-shadow: 1rpx -9rpx 0 1rpx #fff, 2rpx -19rpx 0 2rpx #fff, 3rpx -29rpx 0 3rpx #fff;
}

.emoji--like .emoji__thumb {
  border-bottom: 20rpx solid #fff;
  border-left: 20rpx solid transparent;
  top: -25rpx;
  right: -25rpx;
  z-index: 2;
  transform: rotate(5deg);
  transform-origin: 0% 100%;
  animation: thumbs-up 2s linear infinite;
}

.emoji--like .emoji__thumb:before {
  border-radius: 50% 50% 0 0;
  background: #fff;
  width: 10rpx;
  height: 12rpx;
  left: -10rpx;
  top: -8rpx;
  transform: rotate(-15deg);
  transform-origin: 100% 100%;
  box-shadow: -1rpx 4rpx 0 -1rpx #fff;
}

.emoji--love {
  background: #f55064;
}

.emoji--love:after {
  content: "收藏";
}

.emoji--love .emoji__heart {
  left: calc(50% - 40rpx);
  top: calc(50% - 40rpx);
  width: 80rpx;
  height: 80rpx;
  animation: heart-beat 1s linear infinite alternate;
}

.emoji--love .emoji__heart:before, .emoji--love .emoji__heart:after {
  left: calc(50% - 20rpx);
  top: calc(50% - 32rpx);
  width: 40rpx;
  height: 64rpx;
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
}

.emoji--love .emoji__heart:before {
  transform: translate(20rpx) rotate(-45deg);
  transform-origin: 0 100%;
}

.emoji--love .emoji__heart:after {
  transform: translate(-20rpx) rotate(45deg);
  transform-origin: 100% 100%;
}

.emoji--haha:after {
  content: "大笑";
}

.emoji--haha .emoji__face {
  animation: haha-face 2s linear infinite;
}

.emoji--haha .emoji__eyes {
  width: 26rpx;
  height: 6rpx;
  border-radius: 2rpx;
  left: calc(50% - 13rpx);
  top: 35rpx;
  transform: rotate(20deg);
  background: transparent;
  box-shadow: -25rpx 5rpx 0 0 #000, 25rpx -5rpx 0 0 #000;
}

.emoji--haha .emoji__eyes:after {
  left: 0;
  top: 0;
  width: 26rpx;
  height: 6rpx;
  border-radius: 2rpx;
  transform: rotate(-40deg);
  background: transparent;
  box-shadow: -25rpx -5rpx 0 0 #000, 25rpx 5rpx 0 0 #000;
}

.emoji--haha .emoji__mouth {
  width: 80rpx;
  height: 40rpx;
  left: calc(50% - 40rpx);
  top: 50%;
  background: #000;
  border-radius: 0 0 40rpx 40rpx;
  overflow: hidden;
  z-index: 1;
  animation: haha-mouth 2s linear infinite;
}

.emoji--haha .emoji__tongue {
  width: 70rpx;
  height: 30rpx;
  background: #f55064;
  left: calc(50% - 35rpx);
  bottom: -10rpx;
  border-radius: 50%;
}

.emoji--yay:after {
  content: "Yay";
  animation: yay-reverse 1s linear infinite;
}

.emoji--yay .emoji__face {
  animation: yay 1s linear infinite alternate;
}

.emoji--yay .emoji__eyebrows {
  left: calc(50% - 3rpx);
  top: 30rpx;
  height: 6rpx;
  width: 6rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: -6rpx 0 0 0 #000, -36rpx 0 0 0rpx #000, 6rpx 0 0 0 #000, 36rpx 0 0 0rpx #000;
}

.emoji--yay .emoji__eyebrows:before, .emoji--yay .emoji__eyebrows:after {
  width: 36rpx;
  height: 18rpx;
  border-radius: 60rpx 60rpx 0 0;
  background: transparent;
  border: 6rpx solid black;
  box-sizing: border-box;
  border-bottom: 0;
  bottom: 3rpx;
  left: calc(50% - 18rpx);
}

.emoji--yay .emoji__eyebrows:before {
  margin-left: -21rpx;
}

.emoji--yay .emoji__eyebrows:after {
  margin-left: 21rpx;
}

.emoji--yay .emoji__mouth {
  top: 60rpx;
  background: transparent;
  left: 50%;
}

.emoji--yay .emoji__mouth:after {
  width: 80rpx;
  height: 80rpx;
  left: calc(50% - 40rpx);
  top: -75rpx;
  border-radius: 50%;
  background: transparent;
  border: 6rpx solid #000;
  box-sizing: border-box;
  border-top-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  z-index: 1;
}

.emoji--yay .emoji__mouth:before {
  width: 6rpx;
  height: 6rpx;
  background: transparent;
  border-radius: 50%;
  bottom: 5rpx;
  left: calc(50% - 3rpx);
  box-shadow: -25rpx 0 0 0 #000,
                25rpx 0 0 0 #000,
                -35rpx -2rpx 30rpx 10rpx #d5234c,
                35rpx -2rpx 30rpx 10rpx #d5234c;
}

.emoji--wow:after {
  content: "惊讶";
}

.emoji--wow .emoji__face {
  animation: wow-face 3s linear infinite;
}

.emoji--wow .emoji__eyebrows {
  left: calc(50% - 3rpx);
  height: 6rpx;
  width: 6rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: -18rpx 0 0 0 #000, -33rpx 0 0 0 #000, 18rpx 0 0 0 #000, 33rpx 0 0 0 #000;
  animation: wow-brow 3s linear infinite;
}

.emoji--wow .emoji__eyebrows:before, .emoji--wow .emoji__eyebrows:after {
  width: 24rpx;
  height: 20rpx;
  border: 6rpx solid #000;
  box-sizing: border-box;
  border-radius: 50%;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  top: -3rpx;
  left: calc(50% - 12rpx);
}

.emoji--wow .emoji__eyebrows:before {
  margin-left: -25rpx;
}

.emoji--wow .emoji__eyebrows:after {
  margin-left: 25rpx;
}

.emoji--wow .emoji__eyes {
  width: 16rpx;
  height: 24rpx;
  left: calc(50% - 8rpx);
  top: 35rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: 25rpx 0 0 0 #000, -25rpx 0 0 0 #000;
}

.emoji--wow .emoji__mouth {
  width: 30rpx;
  height: 45rpx;
  left: calc(50% - 15rpx);
  top: 50%;
  border-radius: 50%;
  background: #000;
  animation: wow-mouth 3s linear infinite;
}

.emoji--sad:after {
  content: "无感";
}

.emoji--sad .emoji__face {
  animation: sad-face 2s ease-in infinite;
}

.emoji--sad .emoji__eyebrows {
  left: calc(50% - 3rpx);
  top: 35rpx;
  height: 6rpx;
  width: 6rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: -40rpx 9rpx 0 0 #000, -25rpx 0 0 0 #000, 25rpx 0 0 0 #000, 40rpx 9rpx 0 0 #000;
}

.emoji--sad .emoji__eyebrows:before, .emoji--sad .emoji__eyebrows:after {
  width: 30rpx;
  height: 20rpx;
  border: 6rpx solid #000;
  box-sizing: border-box;
  border-radius: 50%;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  top: 2rpx;
  left: calc(50% - 15rpx);
}

.emoji--sad .emoji__eyebrows:before {
  margin-left: -30rpx;
  transform: rotate(-30deg);
}

.emoji--sad .emoji__eyebrows:after {
  margin-left: 30rpx;
  transform: rotate(30deg);
}

.emoji--sad .emoji__eyes {
  width: 14rpx;
  height: 16rpx;
  left: calc(50% - 7rpx);
  top: 50rpx;
  border-radius: 50%;
  background: transparent;
  box-shadow: 25rpx 0 0 0 #000, -25rpx 0 0 0 #000;
}

